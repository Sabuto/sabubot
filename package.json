{
  "name": "sabubot",
  "version": "1.0.0",
  "private": true,
  "description": "My personal bot",
  "author": "Robert Dunne <robe_dunne@hotmail.com>",
  "license": "ISC",
  "repository": "https://github.com/Sabuto/sabubot.git",
  "homepage": "https://github.com/Sabuto/sabubot",
  "bugs": "https://github.com/Sabuto/sabubot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "9.6.6",
    "ignore": "5.1.4",
    "js-yaml": "3.13.1",
    "normalize-for-search": "2.1.0"
  },
  "devDependencies": {
    "jest": "24.9.0",
    "nock": "11.7.0",
    "nodemon": "1.19.4",
    "smee-client": "1.1.0",
    "standard": "14.3.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
